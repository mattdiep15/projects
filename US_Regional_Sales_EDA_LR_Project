       "Pipeline(steps=[('preprocessor',\n",
       "                 ColumnTransformer(transformers=[('num', StandardScaler(),\n",
       "                                                  ['discount_applied',\n",
       "                                                   'unit_cost', 'unit_price',\n",
       "                                                   'order_month', 'order_day',\n",
       "                                                   'order_year',\n",
       "                                                   'delivery_duration',\n",
       "                                                   'ship_delay',\n",
       "                                                   'profit_margin']),\n",
       "                                                 ('cat',\n",
       "                                                  OneHotEncoder(handle_unknown='ignore'),\n",
       "                                                  ['sales_channel',\n",
       "                                                   'store_id'])])),\n",
       "                ('regressor', LinearRegression())])"
      ]
