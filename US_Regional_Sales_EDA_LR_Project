   "source": [
    "# put dates into proper format and aggregate by month\n",
    "df['OrderDate'] = pd.to_datetime(df['OrderDate'])\n",
    "df['OrderMonth'] = df['OrderDate'].dt.to_period('M').astype(str)\n",
    "\n",
    "# group data by month keeping total rev and order quantity\n",
    "monthly_summary = df.groupby('OrderMonth').agg({\n",
    "    'Total_Revenue': 'sum',\n",
    "    'Order Quantity': 'sum'\n",
    "}).reset_index()"
