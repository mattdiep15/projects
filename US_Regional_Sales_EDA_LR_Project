   "source": [
    "# extract time based features\n",
    "df['order_month'] = df['order_date'].dt.month\n",
    "df['order_day'] = df['order_date'].dt.weekday\n",
    "df['order_year'] = df['order_date'].dt.year\n",
    "\n",
    "df['delivery_duration'] = (df['delivery_date'] - df['order_date']).dt.days\n",
    "df['ship_delay'] = (df['ship_date'] - df['order_date']).dt.days\n",
    "\n",
    "# price based features\n",
    "df['total_cost'] = df['unit_cost'] * df['order_quantity']\n",
    "df['total_revenue'] = df['unit_price'] * df['order_quantity']\n",
    "df['discount_amount'] = df['unit_price'] * df['discount_applied'] * df['order_quantity']\n",
    "df['profit'] = df['total_revenue'] - df['total_cost']\n",
    "df['profit_margin'] = df['profit'] / df['total_revenue']"
   ]
  },
