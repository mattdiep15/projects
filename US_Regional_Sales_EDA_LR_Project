    "# new total revenue column\n",
    "df['Total_Revenue'] = df['Order Quantity'] * df['Unit Price']\n",
    "\n",
    "# revenue grouped by warehouse\n",
    "warehouse_sales = df.groupby('WarehouseCode')['Total_Revenue'].sum().sort_values(ascending=False)\n",
    "warehouse_sales_df = warehouse_sales.reset_index()\n",
    "\n",
    "# plot\n",
    "plt.figure(figsize=(10,5))\n",
    "sns.barplot(\n",
    "    data=warehouse_sales_df,\n",
    "    x= 'WarehouseCode',\n",
    "    y= 'Total_Revenue',\n",
    "    palette='crest'\n",
    ")\n",
    "\n",
    "plt.xticks(rotation= 45)\n",
    "plt.title('Total Revenue by Warehouse')\n",
    "plt.xlabel('Warehouse Code')\n",
    "plt.ylabel('Total Revenue (Millions of USD)')"
