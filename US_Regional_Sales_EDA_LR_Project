    "# filter for 2020 only\n",
    "df_2020 = df[df['OrderDate'].dt.year == 2020]\n",
    "df_2020['MonthName'] = df_2020['OrderDate'].dt.strftime('%B')\n",
    "df_2020['MonthNumber'] = df_2020['OrderDate'].dt.month  # for sorting\n",
    "\n",
    "# group by month\n",
    "monthly_channel_counts = (\n",
    "    df_2020.groupby(['MonthNumber', 'MonthName', 'Sales Channel'])\n",
    "    .size()\n",
    "    .reset_index(name='Order_Count')\n",
    "    .sort_values('MonthNumber') \n",
    ")\n",
    "\n",
    "channel_order = ['In-Store', 'Online', 'Distributor', 'Wholesale']\n",
    "months = monthly_channel_counts['MonthName'].unique()\n",
    "fig, axes = plt.subplots(3, 4, figsize=(20, 10))\n",
    "axes = axes.flatten()\n",
    "\n",
    "# loop over each month and make a barplot\n",
    "for i, month in enumerate(months):\n",
    "    ax = axes[i]\n",
    "    data = monthly_channel_counts[monthly_channel_counts['MonthName'] == month]\n",
    "\n",
    "    sns.barplot(\n",
    "        data=data,\n",
    "        x='Sales Channel',\n",
    "        y='Order_Count',\n",
    "        ax=ax,\n",
    "        palette='crest',\n",
    "        order=channel_order\n",
    "    )\n",
    "\n",
    "    ax.set_title(month)\n",
    "    ax.set_xlabel('')\n",
    "    ax.set_ylabel('Orders')\n",
    "    ax.tick_params(axis='x', rotation=30)\n",
    "\n",
    "\n",
    "for j in range(i + 1, len(axes)):\n",
    "    fig.delaxes(axes[j])\n",
    "\n",
    "fig.suptitle('Monthly Order Volume by Sales Channel (2020)', fontsize=18)\n",
    "plt.tight_layout()\n",
    "plt.subplots_adjust(top=0.9)"
   ]
